{
  "expo": {
    "name": "group-decision-making-app",
    "slug": "group-decision-making-app",
    "scheme": "aora",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/cooperation.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/cooperation.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/cooperation.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "favicon": "./assets/cooperation.png"
    },
    "plugins": [
      "expo-router",
      "expo-font"
    ],
    "expo": {
      "assetBundlePatterns": ["**/*"]
    }
  }
}
